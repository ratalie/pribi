{
  "name": "probo-v3",
  "type": "module",
  "private": true,
  "scripts": {
    "typecheck": "nuxi typecheck",
    "typecheck:tests": "vue-tsc --noEmit --project tsconfig.tests.json",
    "typecheck:all": "npm run typecheck && npm run typecheck:tests",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.vue --fix",
    "build": "npm run typecheck && npm run lint && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "// ===== SISTEMA 1: SUITE MAESTRA (tests/) =====": "",
    "test:suite:flujo-completo": "TEST_USE_MSW=false vitest run tests/sociedades/flujo-completo-sociedades.test.ts",
    "test:suite:paso0": "TEST_USE_MSW=false vitest run tests/sociedades/paso-0-crear-sociedad.test.ts",
    "test:suite:directorio": "TEST_USE_MSW=false vitest run tests/sociedades/paso-5-directorio.test.ts",
    "test:suite:apoderados": "TEST_USE_MSW=false vitest run tests/sociedades/paso-6-apoderados.test.ts",
    "test:suite:quorum": "TEST_USE_MSW=false vitest run tests/sociedades/paso-8-quorum.test.ts",
    "// ===== SISTEMA 2A: TESTS CON BACKEND REAL =====": "",
    "test:core:sociedad": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/infrastructure/repositories/__tests__/",
    "test:core:datos-sociedad": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/datos-sociedad",
    "test:core:accionistas": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/accionistas",
    "test:core:acciones": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/acciones",
    "test:core:asignacion": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/asignacion-acciones",
    "test:core:directorio": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/directorio/infrastructure/repositories/__tests__/directorio.test.ts",
    "test:core:apoderados": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/apoderados/infrastructure/repositories/__tests__/apoderados.test.ts",
    "test:core:quorum": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades/pasos/quorum-mayorias/infrastructure/repositories/__tests__/quorum.test.ts",
    "test:core:all": "TEST_USE_MSW=false vitest run app/core/hexag/registros/sociedades",
    "// ===== SISTEMA 2B: TESTS CON MSW (SIN BACKEND) =====": "",
    "test:core:sociedad:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/infrastructure/repositories/__tests__/",
    "test:core:datos-sociedad:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/datos-sociedad",
    "test:core:accionistas:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/accionistas",
    "test:core:acciones:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/acciones",
    "test:core:asignacion:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/asignacion-acciones",
    "test:core:directorio:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/directorio/infrastructure/repositories/__tests__/directorio.test.ts",
    "test:core:apoderados:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/apoderados/infrastructure/repositories/__tests__/apoderados.test.ts",
    "test:core:quorum:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades/pasos/quorum-mayorias/infrastructure/repositories/__tests__/quorum.test.ts",
    "test:core:all:msw": "TEST_USE_MSW=true vitest run app/core/hexag/registros/sociedades",
    "// ===== SISTEMA 3: TESTS JUNTAS =====": "",
    "test:juntas:all": "TEST_USE_MSW=false vitest run app/core/hexag/juntas",
    "test:juntas:all:msw": "TEST_USE_MSW=true vitest run app/core/hexag/juntas",
    "test:juntas:paso-0": "TEST_USE_MSW=false vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/junta.repository.shared.test.ts",
    "test:juntas:paso-0:msw": "TEST_USE_MSW=true vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/junta.repository.shared.test.ts",
    "test:juntas:snapshot": "TEST_USE_MSW=false vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/junta-snapshot.test.ts",
    "test:juntas:seleccion-agenda": "TEST_USE_MSW=false vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*agenda*.test.ts",
    "test:juntas:seleccion-agenda:msw": "TEST_USE_MSW=true vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*agenda*.test.ts",
    "test:juntas:detalles": "TEST_USE_MSW=false vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*meeting*.test.ts",
    "test:juntas:detalles:msw": "TEST_USE_MSW=true vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*meeting*.test.ts",
    "test:juntas:instalacion": "TEST_USE_MSW=false vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*asistencia*.test.ts",
    "test:juntas:instalacion:msw": "TEST_USE_MSW=true vitest run app/core/hexag/juntas/infrastructure/repositories/__tests__/*asistencia*.test.ts",
    "test:juntas:watch": "vitest watch app/core/hexag/juntas",
    "// ===== UTILS =====": "",
    "test:cleanup": "TEST_USE_MSW=false vitest run cleanup.test.ts",
    "test:all:sociedades": "npm run test:suite:flujo-completo && npm run test:core:all",
    "test:all:juntas": "npm run test:juntas:all",
    "test:all": "npm run test:all:sociedades && npm run test:all:juntas",
    "// ===== LEGACY (compatibilidad) =====": "",
    "test:sociedades:flujo-completo": "npm run test:suite:flujo-completo",
    "test:sociedades:backend": "npm run test:suite:flujo-completo"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.12.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "^10.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@tanstack/vue-table": "^8.21.3",
    "@types/lodash-es": "^4.17.12",
    "@unhead/vue": "^2.0.19",
    "@unovis/ts": "^1.6.2",
    "@unovis/vue": "^1.6.2",
    "@vee-validate/i18n": "^4.15.1",
    "@vee-validate/rules": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx-preview": "^0.3.7",
    "docxtemplater": "^3.67.5",
    "echarts": "^6.0.0",
    "eslint": "^9.37.0",
    "floating-vue": "^5.2.2",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.545.0",
    "motion-v": "^1.7.4",
    "nuxt": "^4.1.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "pizzip": "^3.2.0",
    "reka-ui": "^2.6.1",
    "shadcn-nuxt": "^2.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.76",
    "@iconify-json/mdi": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.10.1",
    "@types/pizzip": "^3.0.5",
    "@vitejs/plugin-vue": "^6.0.2",
    "jsdom": "^27.2.0",
    "msw": "^2.12.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
