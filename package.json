{
  "name": "probo-v3",
  "type": "module",
  "private": true,
  "scripts": {
    "typecheck": "nuxi typecheck",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.vue --fix",
    "build": "npm run typecheck && npm run lint && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:integration": "TEST_USE_MSW=false vitest run",
    "test:msw": "TEST_USE_MSW=true vitest",
    "test:sociedades": "TEST_USE_MSW=false vitest run sociedad.repository.integration.test.ts",
    "test:registros": "TEST_USE_MSW=false vitest run datos-sociedad.repository.integration.test.ts accionistas.repository.integration.test.ts acciones.repository.integration.test.ts asignacion-acciones.repository.integration.test.ts quorum.repository.integration.test.ts director.repository.integration.test.ts apoderados.repository.integration.test.ts",
    "test:registros:all": "TEST_USE_MSW=false vitest run sociedad.repository.integration.test.ts datos-sociedad.repository.integration.test.ts accionistas.repository.integration.test.ts acciones.repository.integration.test.ts asignacion-acciones.repository.integration.test.ts quorum.repository.integration.test.ts director.repository.integration.test.ts apoderados.repository.integration.test.ts"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.12.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "^10.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@tanstack/vue-table": "^8.21.3",
    "@types/lodash-es": "^4.17.12",
    "@unhead/vue": "^2.0.19",
    "@vee-validate/i18n": "^4.15.1",
    "@vee-validate/rules": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.37.0",
    "floating-vue": "^5.2.2",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.545.0",
    "motion-v": "^1.7.4",
    "nuxt": "^4.1.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "reka-ui": "^2.6.0",
    "shadcn-nuxt": "^2.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.76",
    "@iconify-json/mdi": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "jsdom": "^27.2.0",
    "msw": "^2.12.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}